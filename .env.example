# OpenAI
OPENAI_API_KEY=sk-...

# Neo4j (reuse temporal-tb-neo4j container)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password

# Indexing
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
SKELETON_BETA=0.25

# Retrieval
TOP_K_VECTOR=10
TOP_K_FINAL=5
VECTOR_THRESHOLD=0.7
MAX_HOPS=2

# Agent
MAX_RETRIES=2
RELEVANCE_THRESHOLD=3.0

# LLM
LLM_MODEL=gpt-4o
LLM_MODEL_MINI=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIM=1536
