# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_LLM_MODEL=gpt-4o
OPENAI_LLM_MODEL_MINI=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=1536
# OPENAI_BASE_URL=http://localhost:4000/v1  # Uncomment for LiteLLM proxy

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password_here

# Indexing
INDEXING_CHUNK_SIZE=1000
INDEXING_CHUNK_OVERLAP=200
INDEXING_SKELETON_BETA=0.25
INDEXING_KNN_K=10
INDEXING_PAGERANK_DAMPING=0.85

# Retrieval
RETRIEVAL_TOP_K_VECTOR=10
RETRIEVAL_TOP_K_FINAL=10
RETRIEVAL_VECTOR_THRESHOLD=0.5
RETRIEVAL_MAX_HOPS=3

# Agent
AGENT_MAX_RETRIES=2
AGENT_RELEVANCE_THRESHOLD=2.0

# Logging
LOG_LEVEL=INFO
